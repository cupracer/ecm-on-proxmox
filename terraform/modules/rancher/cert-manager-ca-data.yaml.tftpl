apiVersion: v1
kind: Secret
metadata:
  name: private-ca-secret
  namespace: cert-manager
data:
  tls.crt: "${cert_data}"
  tls.key: "${key_data}"
type: kubernetes.io/tls
---
apiVersion: cert-manager.io/v1
kind: ClusterIssuer
metadata:
  name: private-ca-issuer
spec:
  ca:
    secretName: private-ca-secret

